<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="BrandCollection">
        <property name="brands" type="Brand[]" singular="brand"/>
    </transfer>

    <transfer name="Brand">
        <property name="brandCustomerRelation" type="BrandCustomerRelation"/>
    </transfer>

    <transfer name="BrandCustomerRelation">
        <property name="idBrand" type="int"/>
        <property name="customerIds" type="int[]"/>
    </transfer>

    <transfer name="Customer">
        <property name="brandCollection" type="CustomerBrandCollection"/>
    </transfer>
</transfers>
